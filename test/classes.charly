class List {
    let data = [];

    func push(arg) {
        data = append(data, arg);
    };

    func pop() {
        let new_data = [];
        let popped = null;
        each(data, func(value, index) {
            if (index ! (length(data) - 1)) {
                new_data = append(new_data, value);
            } else {
                popped = value;
            };
        });
        data = new_data;
        popped;
    };
};

let myList = new(List);
let push = memberex(myList, "push");
let pop = memberex(myList, "pop");

push("!");
push("World");
push("Hello");

print(pop()); # Hello
print(pop()); # World
print(pop()); # !
print(pop()); # NULL
