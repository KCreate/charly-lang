const length = __internal__method("length")
const _colorize = __internal__method("colorize")
const typeof = __internal__method("typeof")

export = primitive class Object {
  func length() {
    length(self)
  }

  func to_s() {
    "" + self;
  }

  func colorize(code) {
    _colorize(self.to_s(), code)
  }

  func typeof() {
    typeof(self)
  }
}
