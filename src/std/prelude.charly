class __container_primitive {}

primitive class Object {
  func tap(callback) {
    callback(self);
    self
  }
}

primitive class Numeric {
  func increment() {
    self + 1
  }
}

class Exception {
  property message

  func constructor(message) {
    @message = message
  }

  func to_s() {
    "test"
  }
}
