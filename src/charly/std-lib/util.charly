func repeat(amount, callback) {
  let i = 0;
  while (i < amount) {
    callback(i);
    i = i + 1;
  };
};

func to_numeric(arg) {
  call_internal("to_numeric", arg);
};

func trim(arg) {
  call_internal("trim", arg);
};

