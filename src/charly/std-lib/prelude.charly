func include(filename) {
  call_internal("include", filename);
};

func require(filename) {
  call_internal("require", filename);
};

func unpack(object) {
  call_internal("unpack", object);
};

# Array functions
unpack(require("array"));

# Default print and write functions
unpack(require("io"));
let print = stdout.print;
let write = stdout.write;
let gets = stdin.gets;
let getc = stdin.getc;
